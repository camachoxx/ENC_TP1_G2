simbin3<-function(samples,n,p){
cp<-vector()
s<-vector()
x<-vector()
for (i in 0:n){
pj=bincoef(n,i)*(p^i)*((1-p)^(n-i))
s=c(s,pj)
cp<-c(cp,sum(s))
}
print(s)
print(cp)
while(samples>=1){
for (i in 1:length(cp)){
u=runif(1,0,1)
if(u<cp[i]){
x=c(x,i-1)
samples=samples-1
}
}
}
return(x)
}
