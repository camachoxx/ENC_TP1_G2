#Exercicio A
sim.norm<-function(n,m,std){
x<-vector()
y<-vector()
for (i in 1:n){
ome=2*pi*runif(1,0,1)
R=sqrt(-2*log(runif(1,0,1)))
x=c(x,R*cos(ome))
y=c(y,R*sin(ome))
}
x=x*std+m
y=y*std+m
return(list(x,y))
}
